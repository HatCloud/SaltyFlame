{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "build",
    "public",
    "scripts",
    "webpack",
    "jest",
    "tests",
    "e2e",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
